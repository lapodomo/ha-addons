{
  "init": "false",
  "name": "TransIP dyndns",
  "version": "1.3.3",
  "slug": "transipdns",
  "hassio_api": true,
  "description": "Dynamic DNS via TransIP with Let's Encrypt support",
  "url": "https://github.com/lapo/ha-addons",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "map": ["ssl:rw"],
  "options": {
    "lets_encrypt": {
      "accept_terms": true,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem"
    },
   
    "username":null,
    "domain": "",
    "aliases": [],
    "updatedns": false,
    "seconds": 300
  },
  "schema": {
    "lets_encrypt": {
      "accept_terms": "bool",
      "certfile": "str",
      "keyfile": "str"
    },
    "ipv4": "str?",
    "ipv6": "str?",
    "updatedns": "bool",
    "username": "str",
    "domain": "str",
    "aliases": [
      {"domain": "str", "alias": "str"}
    ],
    "seconds": "int"
  }
}
